#include <iostream>
using namespace std;

struct Item {
    int id;
    float cost;
}items[6];

int main() {
    items[0] = {1, 60.50};
    items[1] = {2, 40.75};
    items[2] = {3, 120.30};
    items[3] = {4, 55.00};
        
    for (int i = 0; i < 6; i++) {
        if (items[i].cost > 50.00) {
            cout << "Item ID: " << items[i].id << ", Cost: " << items[i].cost << endl;
        }
    }

    return 0;
}
